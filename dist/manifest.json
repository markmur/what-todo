{
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "name": "What Todo",
  "description": "A Chrome Extension to manage your workload in the New Tab screen",
  "version": "2.0.0",
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "extension.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "extension.html"
  }
}
